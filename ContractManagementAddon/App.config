<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8" />
  </startup>
  <appSettings>
    <!-- Path to SAP SDK (relative to addon directory or absolute path)
         If not specified, the addon will search in standard locations -->
    <add key="SapSdkDir" value="" />

    <!-- Enable debug logging -->
    <add key="EnableDebugLog" value="true" />

    <!-- Log file location (relative to addon directory)
         If not specified, logs go to addon directory -->
    <add key="LogFilePath" value="ContractManagementAddon.log" />
  </appSettings>
</configuration>
